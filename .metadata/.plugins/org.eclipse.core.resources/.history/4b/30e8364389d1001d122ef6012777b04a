package com.masai;

import java.io.Serializable;

public class CrimeDetails implements Serializable{

	private String name;
	private String type;
	private String ps_area;
	private String victumName;

	public CrimeDetails() {
		super();
	}

	public CrimeDetails(String name, String type, String ps_area, String victumName) {
		super();
		this.name = name;
		this.type = type;
		this.ps_area = ps_area;
		this.victumName = victumName;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getPs_area() {
		return ps_area;
	}

	public void setPs_area(String ps_area) {
		this.ps_area = ps_area;
	}

	public String getVictumName() {
		return victumName;
	}

	public void setVictumName(String victumName) {
		this.victumName = victumName;
	}

	@Override
	public String toString() {
		return "User [Criminal name=" + name + ", type=" + type + ", Crime_area=" + ps_area + ", victumName=" + victumName
				+ "]";
	}

}

